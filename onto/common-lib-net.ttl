s-wf:UserTaskFormTemplate
  rdf:type v-ui:ClassTemplate ;
  rdfs:isDefinedBy s-wf: ;
  rdfs:label "Шаблон для класса Форма ответа на задачу"@ru ;
  v-ui:forClass s-wf:UserTaskForm ;
  v-ui:template """
<script>
  // Render possible decisions radios
  var radioTmpl = $(".radio.decision", template);
  if (individual.hasValue("v-wf:possibleDecisionClass")) {
    individual["v-wf:possibleDecisionClass"].map( function (value, index) {
      var tmpl = radioTmpl.clone().insertBefore(radioTmpl);
      var input = $("input", tmpl).val(index);
      $("label", tmpl).append("<span about='" + value.id + "' property='rdfs:label'></span>");
      if (
        individual.hasValue("v-wf:takenDecision")
        && individual["v-wf:takenDecision"][0]["rdf:type"][0].id === individual["v-wf:possibleDecisionClass"][index].id
      ) {
        input.prop("checked", true);
      }
    });
  }
  radioTmpl.remove();

  //# sourceURL=s-wf:UserTaskFormTemplate_pre
</script>
<div class="container">
  <div class="sheet clearfix">
    <h2 about="@" property="rdfs:label"></h2>
    <div class="row">
      <div class="col-md-3">
        <em about="v-wf:from" property="rdfs:label"></em>
        <div about="@" rel="v-wf:from" data-template="v-ui:LabelTemplate"></div>
      </div>
      <div class="col-md-3">
         <em about="v-wf:to" property="rdfs:label"></em>
         <div about="@" rel="v-wf:to" data-template="v-ui:LabelTemplate"></div>
      </div>
      <div class="col-md-3">
        <em about="v-s:description" property="rdfs:label"></em>
        <div about="@" property="v-s:description"></div>
      </div>
      <div class="col-md-3">
        <em about="v-wf:dateGiven" property="rdfs:label"></em>
        <div about="@" property="v-wf:dateGiven"></div>
      </div>
    </div>
    <div about="@" rel="v-wf:redirect_from_task">
      <hr>
      <div class="row">
        <div class="col-md-3">
          <em about="v-s:TaskIsRedirectedFrom" property="rdfs:label"> </em>
          <span rel="v-wf:to" data-template="v-ui:LabelTemplate"></span>
          <!--span rel="v-s:creator v-wf:from" data-template="v-ui:LabelTemplate"></span-->
        </div>
        <div rel="v-wf:takenDecision" class="col-md-3">
          <em about="rdfs:comment" property="rdfs:label"> </em>
          <span about="@"  property="rdfs:comment"></span>
        </div>
      </div>
    </div>
    <hr />
    <em about="v-wf:takenDecision" property="rdfs:label"></em>
    <div class="radio decision">
      <label>
        <input type="radio" name="decisionRadios">
      </label>
    </div>
    <div about="@" rel="v-wf:takenDecision"></div>
    <div id="newDecisionHolder"></div>
    <!--div class="actions view edit -search pull-right">
      <div class="btn-group dropup">
        <button class="btn btn-warning dropdown-toggle sendbutton view -edit -search" data-toggle="dropdown">
          <span about="v-s:SendTask" property="rdfs:label"> </span>
          <span class="caret"></span>
        </button>
        <ul class="dropdown-menu" id="standard-tasks">
        </ul>
      </div>
    </div-->
  </div>
  <br>
  <div about="@" class="view edit -search sheet" data-template="v-ft:TaskRelatedTasksTemplate"></div>
  <br>
  <div about="@" rel="v-wf:onDocument" class="view edit -search sheet"></div>
</div>
<script>
  $(".radio.decision input", template).change(function () {
    var type = individual["v-wf:possibleDecisionClass"][$(this).val()];
    var decision = createDecision(type);
    var holder = $("#newDecisionHolder", template).empty();
    decision.present(holder, undefined, "edit");
    // Disable decision radios a decision is saved
    decision.one("afterSave", disableRadios);
    template.one("remove", function () {
      decision.off("afterSave", disableRadios);
    });
  });

  decisionHandler();

  individual.on("v-wf:isCompleted", decisionHandler);
  template.one("remove", function () {
    individual.off("v-wf:isCompleted", decisionHandler);
  });

  function disableRadios () {
    $(".radio.decision", template).addClass("disabled").find("input").prop("disabled", true);
  }
  function createDecision(type) {
    var decision = new veda.IndividualModel();
    decision["rdf:type"] = [ type ];
    decision.properties["rdfs:label"] = type.properties["rdfs:label"];
    decision["v-s:backwardTarget"] = [ individual ];
    decision["v-s:backwardProperty"] = [ new veda.IndividualModel("v-wf:takenDecision") ];
    return decision;
  }
  function decisionHandler () {
    var isMine = (veda.appointment && veda.appointment.id === individual["v-wf:to"][0].id)
              || (veda.appointment && veda.appointment.hasValue("v-s:occupation") && veda.appointment["v-s:occupation"][0].id === individual["v-wf:to"][0].id)
              || (veda.user && veda.user.id === individual["v-wf:to"][0].id)
              || individual.rights.hasValue("v-s:canUpdate", true);
    var isOpen = !individual.hasValue("v-wf:isCompleted", true);
    if (isMine && isOpen) {
      $(".radio.decision", template).removeClass("disabled").find("input").prop("disabled", false);
      $(".radio.decision input", template).first().prop("checked", true).change();
    } else {
      $(".radio.decision", template).addClass("disabled").find("input").prop("disabled", true);
      $("#newDecisionHolder", template).remove();
    }
  }

  //# sourceURL=s-wf:UserTaskFormTemplate_post
</script>
  """ ;
.

