# -------------------------------------------------------------- СОБЫТИЯ --

v-wf:AutoComplitTask_Daily
  rdf:type v-s:Event ;
  v-s:author cfg:VedaSystemAppointment ;
  rdfs:label "Автозакрытие задач" ;
  v-s:triggerByUid cfg:daily ;
  v-s:runAt "V8.LowPriority";
  v-s:script """
print("[v-wf:AutoComplitTask_Daily] ************************** START **************************");

var scriptInfo = get_individual(ticket, 'v-wf:AutoComplitTask_Daily_Info');
if (!scriptInfo) {
  var yesterday = new Date();
  yesterday.setDate(yesterday.getDate() - 1);
  scriptInfo = {
    '@': 'v-wf:AutoComplitTask_Daily_Info',
    'rdf:type': veda.Util.newUri('v-s:CronScriptsInfo'),
    'v-s:lastDateOfExecuting':  veda.Util.newDate(yesterday)
  };
}
var lastDate = new Date(scriptInfo['v-s:lastDateOfExecuting'][0].data);
var today = new Date();
var msInDay = 86400000;
var datePeriod = [new Date(+lastDate), new Date(+today)];
var queryString = "( 'rdf:type'=='v-wf:DecisionForm' && 'v-wf:isCompleted'=='false' && 'v-wf:isAutoCompleted'=='true'";
queryString = queryString + " && 'v-wf:dateGiven'==["+ datePeriod[0].toISOString() +","+ datePeriod[1].toISOString() +"])";
print(queryString);
var result = query(ticket, queryString).result;
for (var i=0;i<result.length;i++){
  var uri = veda.Util.genUri();
  try {
    var decisionRevoke = {
      "@": uri,
      "rdf:type": veda.Util.newUri("v-wf:DecisionRevokeTask"),
      "v-s:backwardProperty": veda.Util.newUri("v-wf:takenDecision"),
      "v-s:backwardTarget": veda.Util.newUri(result[i]),
      "v-s:canRead": veda.Util.newBool(true),
      "v-s:creator": veda.Util.newUri("cfg:VedaSystemAppointment"),
      "v-wf:hasStatusWorkflow": veda.Util.newUri("v-wf:ToBeSent"),
      "rdfs:label": veda.Util.newStr("Задача отозвана"),
      "rdfs:comment": veda.Util.newStr("Пользователь не выполнил задачу в срок. Задача отозвана автоматически.")
    };
    put_individual(veda.ticket, decisionRevoke, _event_id);
    print("Created: ", uri, ". For document:", result[i]);

    //backward не обрабатывается в event_16 из-за parent_script_id
    var membershipUri = "d:backward_membership_" + uri.split(":").join('_') + '_' + result[i].split(':').join('_');
    var membership = {
      '@' : membershipUri,
      'rdf:type'     : veda.Util.newUri('v-s:Membership'),
      'v-s:memberOf' : veda.Util.newUri(result[i]),
      'v-s:resource' : veda.Util.newUri(uri),
      'rdfs:comment' : veda.Util.newStr('создано v-wf:AutoComplitTask_Daily')
    };
    put_individual(veda.ticket, membership, _event_id);
    print("Created membership: ", membershipUri);
  } catch (err) {
    print(result[i], err);
    continue;
  };
  var set_in_doc = {
    "@": result[i],
    "v-wf:takenDecision": veda.Util.newUri(uri),
  };
  set_in_individual(veda.ticket, set_in_doc, _event_id);
}

scriptInfo['v-s:lastDateOfExecuting'] = veda.Util.newDate(new Date());
put_individual(ticket, scriptInfo, _event_id);

print("[v-wf:AutoComplitTask_Daily] ************************** END **************************");
""" ;
.

s-wf:UserTaskForm_NotActualResponsible_Notification
  rdf:type v-s:Event ;
  v-s:author cfg:VedaSystemAppointment ;
  rdfs:label "Уведомление отправителя задачи о неактуальности-недоступности исполнителя  " ;
  v-s:triggerByType s-wf:UserTaskForm ;
  v-s:runAt "V8.LowPriority";
  v-s:script """
//print("[s-wf:UserTaskForm_NotActualResponsible_Notification] ************************** START **************************");
// if (parent_script_id && parent_script_id != 'cfg:Event_16') {
//   print("Not first class event. Exit.");
//   return;
// };

const taskUri = document['@'];
const task = document;//get_individual(ticket, taskUri);
//print(`${taskUri} updateCounter: ${task['v-s:updateCounter'][0].data}`);

if (veda.Util.hasValue(task, 'v-s:updateCounter') && task['v-s:updateCounter'][0].data > 1) {
  print(`Skip ${taskUri}: updateCounter > 1`);
  return;
}
let label;
if (veda.Util.hasValue(task, 'rdfs:label')) {
  label = task['rdfs:label'].reduce((acc,l) => {
    if (l.lang == undefined || l.lang == 'RU') acc = l.data;
    return acc;
  }, 'empty label');
  if (label.toLowerCase().indexOf('подписать') == -1) {
    print(`Skip ${taskUri}: label.indexOf('подписать') == -1 (${label})`);
    return;
  }
} else {
  print(`Skip ${taskUri}: empty label`);
  return;
}
if (!veda.Util.hasValue(task, 'v-wf:to')) {
  print(`Skip ${taskUri}: empty v-wf:to`);
  return;
}
// 0 - ok
// 1 - deleted
// 2 - absense & not valid
// 3 - not found person uri in v-wf:to
let personState = 3;
// 0 - ok
// 1 - deleted
// 3 - not found position uri in v-wf:to or position is undefined
let positionState = 3;
let personLabel;
let positionLabel;
let dateAbsenceTo;
let isPersonInTo = false;
let isPositionInTo = false;
task['v-wf:to'].forEach(objUri => {
  const obj = get_individual(ticket, objUri.data);
